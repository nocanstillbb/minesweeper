{
  "name": "minesweeper-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme MinesweeperExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\"",
    "windows": "npx @react-native-community/cli run-windows",
    "test:windows": "jest --config jest.config.windows.js"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-windows": "0.79.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@types/react": "^19.0.0",
    "react-native-builder-bob": "^0.38.3",
    "@rnx-kit/jest-preset": "^0.1.17"
  },
  "engines": {
    "node": ">=18"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "MinesweeperExample",
      "namespace": "MinesweeperExample",
      "template": "cpp-app"
    }
  }
}
